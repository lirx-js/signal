{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "declaration": true,
    "declarationDir": "./dist/types",
    "downlevelIteration": false,
    "emitDecoratorMetadata": false,
    "importHelpers": false,
    "noEmitHelpers": false,
    "noLib": false,
    "outDir": "dist",
    "preserveConstEnums": false,
    "pretty": true,
    "removeComments": false,
    "sourceMap": true
  }
}
